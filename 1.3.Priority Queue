#include <iostream>
#include <queue>

using namespace std;

int main()
{
    int n; cin >> n;

    priority_queue<int> q;

    for(int i = 0; i < n; i++)
    {
        int a; cin >> a;
        q.push(a);
    }

    while(!q.empty())
    {
        int x = q.top();
        q.pop();
        cout << x << endl;
    }

    return 0;
}
